pipeline {
    agent none

    stages {
        stage('stage1') {
            agent {label 'testmaster'}
			steps {
				parallel (
					step1:{
					echo 'this is step1'
					sh 'sleep 5'
					},
					step2:{
					echo 'this is step2'
					sh 'sleep 5'
					}
				)
            }
        }
		stage('stage2') {
            agent {label 'slave1'}
			steps {
                echo 'this is stage 2'
				sh 'sleep 5'
            }
        }
    }
}
